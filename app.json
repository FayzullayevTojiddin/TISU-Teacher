{
  "expo": {
    "name": "TISU O'qituvchi",
    "slug": "TISU-Teacher-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpg",
    "scheme": "app.tisu.teacher-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "entryPoint": "./app/App.tsx",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.tisu.teacher"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0047AB",
        "foregroundImage": "./assets/images/logo.jpg"
      },
      "package": "app.tisu.teacher",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo.jpg"
    },
    "splash": {
      "image": "./assets/images/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#0047AB"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0047AB",
          "dark": {
            "image": "./assets/images/logo.jpg",
            "backgroundColor": "#002F6C"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": false,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "ecc971a3-8409-4e16-9c29-c9b5ff1ec0ed"
      }
    },
    "owner": "tfayzullaev"
  }
}
